-
-  if (e == NULL || cb == NULL) {
-    return ARES_EFORMERR;
-  }
-
-  if (event != NULL) {
-    *event = NULL;
-  }
-
-  /* Validate flags */
-  if (fd == ARES_SOCKET_BAD) {
-    if (flags & (ARES_EVENT_FLAG_READ | ARES_EVENT_FLAG_WRITE)) {
-      return ARES_EFORMERR;
-    }
-    if (!(flags & ARES_EVENT_FLAG_OTHER)) {
-      return ARES_EFORMERR;
-    }
-  } else {
-    if (flags & ARES_EVENT_FLAG_OTHER) {
-      return ARES_EFORMERR;
-    }
-  }
