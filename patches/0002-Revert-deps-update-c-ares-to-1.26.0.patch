-  ares__llist_destroy(e->ev_updates);
-  e->ev_updates = NULL;
-
-  ares__htable_asvp_destroy(e->ev_handles);
-  e->ev_handles = NULL;
-
-  if (e->ev_sys->destroy) {
-    e->ev_sys->destroy(e);
-  }
-
-  ares__thread_mutex_destroy(e->mutex);
